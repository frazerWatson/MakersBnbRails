<%= form_for @space do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :details %>
  <%= f.text_field :details%>

  <%= f.label :price%>
  <%= f.number_field :price %>

  <%= f.label :available_from %>
  <%= f.date_field :available_from %>

  <%= f.label :available_to %>
  <%= f.date_field :available_to %>

  <%= f.submit %>
<% end %>
